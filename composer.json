{
    "name": "joeri-abbo/laravel-dashboard-buienradar-tile",
    "description": "Render buienradar tile on your dashboard",
    "keywords": [
        "joeri-abbo",
        "laravel-dashboard",
        "laravel",
        "spatie",
        "laravel"
    ],
    "homepage": "https://github.com/Joeri-Abbo/laravel-dashboard-buienradar-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Joeri Abbo",
            "email": "joeriabbo@hotmail.com",
            "homepage": "https://www.linkedin.com/in/joeri-abbo-43a457144/",
            "role": "Developer"
        }
    ],
    "require": {
        "spatie/laravel-dashboard": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.6.2"
    },
    "autoload": {
        "psr-4": {
            "JoeriAbbo\\Buienradar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoeriAbbo\\Buienradar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoeriAbbo\\Buienradar\\BuienradarServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
